{
    "root": true,
    "env": {
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "script"
    },
    "extends": ["eslint:recommended", "plugin:jsdoc/recommended-error"],
    "plugins": ["jsdoc"],
    "rules": {
        "jsdoc/require-param-description": "off",
        "jsdoc/require-returns-description": "off"
    }
}
