{
	"env": {
		"commonjs": true,
		"es2021": true,
		"node": true
	},
	"extends": "eslint:recommended",
	"overrides": [],
	"parserOptions": {
		"ecmaVersion": "latest"
	},
	"rules": {
		"no-unused-vars": "warn",
		"no-empty": "warn",
		"no-use-before-define": "error",
		"no-case-declarations": "off"
	}
}
